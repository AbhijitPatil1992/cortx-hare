{
  "server": true,
  "watches": [
    {
      "type": "key",
      "key": "leader",
      "args": [
        "/opt/seagate/consul/elect-rc-leader"
      ]
    },
    {
      "type": "service",
      "service": "confd",
      "handler_type": "http",
      "http_handler_config": {
        "path": "http://localhost:8080",
        "method": "POST",
        "timeout": "10s"
      }
    },
    {
      "type": "service",
      "service": "confd",
      "args": [
        "/opt/seagate/consul/watch-service"
      ]
    }
  ],
  "enable_local_script_checks": true,
  "services": []
}
