{-*- dhall -*-}

let NetId = ../NetId/NetId

let Endpoint = ./Endpoint

let Endpoint/tcp : Text -> Natural -> Natural -> Endpoint
  = \(ipaddr : Text)
 -> \(portal : Natural)
 -> \(tmid : Natural)
 ->
    let nid = NetId.tcp { tcp = { ipaddr = ipaddr, mdigit = None Natural } }
    in { nid = nid, portal = portal, tmid = tmid }

in Endpoint/tcp
