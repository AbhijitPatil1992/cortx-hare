{
  "server" : true,
  "watches": [
    {
      "type": "key",
      "key": "leader",
      "args": ["/opt/seagate/consul/elect-rc-leader"]
    }
  ],
  "enable_local_script_checks" : true,
  "services": [
   {
    "id": "fid",
    "name": "confd",
    "address": "@tcp:12345:44",
    "port": 101,
    "checks": [
      {
        "args": ["/opt/seagate/consul/check-confd"],
        "interval": "10s"
      }
    ]
   },
   {
    "id": "fid",
    "name": "ios",
    "address": "@tcp:12345:41",
    "port": 401
   }
  ]
}
